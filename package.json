{
  "name": "sap-flp-tools",
  "version": "1.0.0",
  "description": "sap flp tools",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlternoTeam/sap-flp-tools.git"
  },
  "keywords": [
    "sap",
    "react",
    "flp",
    "crossnav"
  ],
  "author": "hello@alterno.io",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlternoTeam/sap-flp-tools/issues"
  },
  "homepage": "https://github.com/AlternoTeam/sap-flp-tools#readme",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.3"
  }
}